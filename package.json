{
  "name": "jli-front",
  "version": "0.1.0",
  "private": false,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!src/pages/**",
      "!src/configs/**",
      "!src/store/**",
      "!src/theme/**",
      "!src/interfaces/**/*",
      "!src/api/**",
      "!src/navigation/**/*",
      "!src/**/interfaces/**",
      "!src/**/utils/**",
      "!src/**/hooks/**",
      "!src/**/configs/**",
      "!src/index.tsx",
      "!src/App.tsx",
      "!src/RootHooks.tsx",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/reportWebVitals.ts",
      "!src/__test__/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "start:craco": "craco start",
    "start:prod": "yarn build && yarn start:server",
    "start:server": "node server.js",
    "build:prod": "craco build",
    "test": "craco test --env=jsdom  --watchAll=false --config craco.config.js --coverage",
    "test:watch": "craco test --coverage --env=jsdom --verbose=false --watchAll",
    "clear_test": "craco test --clearCache",
    "eject": "craco eject",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@craco/craco": "^6.4.5",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@mui/styled-engine": "^5.10.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@types/react": "^18.0.21",
    "@types/react-avatar-editor": "^13.0.0",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.1.3",
    "formik": "^2.2.9",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.6",
    "typed.js": "^2.0.12",
    "typescript": "^4.8.4",
    "web-vitals": "^3.0.4",
    "yup": "^0.32.11",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storyshots": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-plugin-import": "^2.26.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "storybook-addon-material-ui5": "^1.0.0",
    "storybook-addon-react-router-v6": "^0.2.1",
    "ts-jest": "^29.0.3",
    "dotenv": "^16.0.3"
  },
  "resolutions": {
    "react-test-renderer": "18.1.0"
  },
  "engines": {
    "node": "v16.18.0",
    "npm": "8.19.2"
  }
}
